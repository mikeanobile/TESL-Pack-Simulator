<?xml version='1.0' encoding='utf-8'?>
<widget id="site.tesl.pack" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TESL Pack Simulator</name>
    <description>
        A pack opening simulator for The Elder Scrolls Legends.
    </description>
    <author email="support@phonegap.com" href="http://TESL.SITE">
        Mike Anobile
    </author>
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="phonegap-admob" source="npm" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="orientation" value="landscape" />
</widget>
